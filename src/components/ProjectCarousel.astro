      <!-- Carousel Track -->
      <div class="carousel-track absolute inset-0 grid place-items-center">
        <div class="track-circle absolute   rounded-full w-9/12 aspect-square" style="background-image:radial-gradient(  white 60%, hsl(0, 0%, 89%) 75%);">

        </div>
      </div>

      <!-- Project Items -->
      <div class="project-items absolute inset-0 ">
        {allProjects.map((project, index) => {
          const category = projectsData.categories.find(cat => cat.id === project.categoryId);
          const angle = (index * 360) / allProjects.length;
          const radians = (angle * Math.PI) / 180;
          // Initial positioning - will be recalculated by JavaScript for responsiveness
          const containerWidth = 600;
          const containerHeight = 400;
          const centerX = containerWidth / 2; // 300
          const centerY = containerHeight / 2; // 200
          const radius = 180;
          const x = centerX + radius * Math.cos(radians - Math.PI / 2);
          const y = centerY + radius * Math.sin(radians - Math.PI / 2);

          return (
            <div
              class={`project-item flex flex-col items-center max-w-16 sm:max-w-20 md:max-w-24 gap-0 ${index === 0 ? 'active' : ''}`}
              style={`
                position: absolute;
                left: ${x}px;
                top: ${y}px;
                transform: translate(-50%, -50%) rotate(${angle}deg);
                --project-color: ${category?.color || '#666'};
                color: ${category?.color || '#666'};
              `}
              data-project-id={project.id}
              data-index={index}
            >
                <div class="project-icon-container size-8 sm:size-12 md:size-15 bg-white p-1 sm:p-1.5 md:p-2 rounded-full shadow-md translate-y-4">
                  <img
                    src={category.icon}
                    alt={category.name}
                    class="w-full h-full object-contain rounded-full"
                  />
                </div>
                <svg class="pin-shape w-full max-w-16 sm:max-w-20 md:max-w-2xs max-h-32 sm:max-h-36 md:max-h-40" xmlns="http://www.w3.org/2000/svg" width="164" height="290" fill="currentColor" viewBox="0 0 164 290"> <path d="m1.043 12.678 72.824 269.909c2.342 8.721 14.702 8.7 17.042-.029l72.239-271.41c2.514-9.398-9.827-15.339-15.63-7.505L134.287 21.58c-26.026 35.322-78.753 35.53-105.047.384L16.59 5.09c-5.83-7.784-18.094-1.8-15.576 7.588h.029Z"/></svg>
            </div>
          );
        })}
      </div>



      <!-- Navigation Button -->
      <button
        class="nav-button absolute top-1/3 right-1/4 transform -translate-y-1/2 size-16 bg-white/50 backdrop-blur-sm rounded-full flex items-center justify-center hover:bg-gray-50/50 transition-colors z-20 border-white border drop-shadow-xl"
        id="next-project"
      >
        <svg class="w-6 h-6 text-gray-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
