<style>
  .project-carousel-section {
    min-height: 100vh;

  }

  .carousel-container {
    position: relative;
  }

  .project-item {
    z-index: 0;
    pointer-events: none; /* Make items not clickable */
  }

  .project-item.active {
    z-index: 30;
  }



  .content-card {
    transition: all 0.5s ease-in-out;
  }



  /* Responsive Design */
  @media (max-width: 768px) {
