  @media (max-width: 480px) {
    .carousel-container {
      width: 320px !important;
      height: 240px !important;
    }


    .project-title {
      font-size: 8px !important;
    }

    .project-category {
      font-size: 6px !important;
    }
  }
</style>

<script>
  import projectsData from '../data/projects.json';
  import { gsap } from 'gsap';

  document.addEventListener('DOMContentLoaded', () => {
    const projectItems = document.querySelectorAll('.project-item');
    const nextButton = document.getElementById('next-project');
    const projectTitle = document.getElementById('project-title');
    const projectDescription = document.getElementById('project-description');
    const learnMoreBtn = document.querySelector('.learn-more-btn');
    const donateBtn = document.querySelector('.donate-btn');

    let currentIndex = 0;
    const totalProjects = projectItems.length;
    let isUserInteracting = false;

    // Track current rotations for smooth transitions
    const currentRotations = new Map();

    // Calculate new positions for all projects based on current index
    function updateProjectPositions() {
      const container = document.querySelector('.carousel-container');
      if (!container) return;

      const containerRect = container.getBoundingClientRect();
      const centerX = containerRect.width / 2;
      const centerY = containerRect.height / 2;
      // Responsive radius calculation
      const radius = Math.min(containerRect.width, containerRect.height) * 0.3;

      // Calculate globe rotation (counter-clockwise)
      const globeRotation = -(currentIndex * 360) / totalProjects;
      const globeElement = document.querySelector('.globe-image');

      projectItems.forEach((item, index) => {
        // Calculate the relative position from current index
        const relativeIndex = (index - currentIndex + totalProjects) % totalProjects;
        const targetAngle = (relativeIndex * 360) / totalProjects;
        const radians = (targetAngle * Math.PI) / 180;

        // Calculate position around the center point
        const x = centerX + radius * Math.cos(radians - Math.PI / 2);
        const y = centerY + radius * Math.sin(radians - Math.PI / 2);

        // Get current rotation from GSAP's internal state
        const currentRotation = gsap.getProperty(item, "rotation") || 0;

        // Calculate the shortest rotation path
        let rotationDiff = targetAngle - currentRotation;
        while (rotationDiff > 180) rotationDiff -= 360;
        while (rotationDiff < -180) rotationDiff += 360;

        const finalRotation = currentRotation + rotationDiff;

        // Use GSAP for smooth animations with calculated shortest rotation
        gsap.to(item, {
          left: x,
          top: y,
          rotation: finalRotation,
          duration: 1.2,
          ease: "power1.out",
          transformOrigin: "50% 50%"
        });

        // Add active class to the project at the top (index 0)
        if (relativeIndex === 0) {
          item.classList.add('active');
        } else {
          item.classList.remove('active');
        }
      });

      // Animate globe rotation synchronized with carousel
      if (globeElement) {
        gsap.to(globeElement, {
          rotation: globeRotation,
          duration: 1.2,
          ease: "power1.out"
        });
      }
    }

    // Initialize carousel
    function updateCarousel() {
      updateProjectPositions();

      // Update content with current project
      const currentProject = projectsData.projects[currentIndex];
      const currentCategory = projectsData.categories.find(cat => cat.id === currentProject.categoryId);

      if (currentProject && projectTitle && projectDescription && learnMoreBtn && donateBtn) {
        projectTitle.textContent = currentProject.title;
        projectTitle.style.color = currentCategory?.color || '#666';
        projectDescription.textContent = currentProject.description;
        (learnMoreBtn as HTMLAnchorElement).href = currentProject.learnMoreUrl;
        (donateBtn as HTMLAnchorElement).href = currentProject.donateUrl;
      }
    }

    // Handle window resize for responsive positioning
    let resizeTimeout: NodeJS.Timeout;
    window.addEventListener('resize', () => {
      clearTimeout(resizeTimeout);
      resizeTimeout = setTimeout(() => {
        updateProjectPositions();
      }, 100);
    });

    // Next button functionality
    if (nextButton) {
      nextButton.addEventListener('click', () => {
        // Stop auto-rotation when user interacts
        isUserInteracting = true;
        stopAutoRotation();
        currentIndex = (currentIndex + 1) % totalProjects;
        updateCarousel();

        // Reset interaction flag after animation
        setTimeout(() => {
          isUserInteracting = false;
        }, 1200); // Slightly longer than animation duration
      });
    }

    // Project items are not clickable - only next button and auto-rotation

    // Auto-rotation (optional)
    let autoRotate: NodeJS.Timeout | null = null;

    function startAutoRotation() {
      if (autoRotate) clearInterval(autoRotate);
      autoRotate = setInterval(() => {
        // Only auto-rotate if user is not interacting
        if (!isUserInteracting) {
          currentIndex = (currentIndex + 1) % totalProjects;
          updateCarousel();
        }
      }, 5000);
    }

    function stopAutoRotation() {
      if (autoRotate) {
        clearInterval(autoRotate);
        autoRotate = null;
      }
    }

    startAutoRotation();

    // Pause auto-rotation on hover
    const carouselContainer = document.querySelector('.carousel-container');
    if (carouselContainer) {
      carouselContainer.addEventListener('mouseenter', () => {
        stopAutoRotation();
      });

      carouselContainer.addEventListener('mouseleave', () => {
        // Add delay before restarting to prevent conflicts
        setTimeout(() => {
          startAutoRotation();
        }, 2000); // 2 second delay
      });
    }

    // Initialize
    updateCarousel();
  });
</script>
